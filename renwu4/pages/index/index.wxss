/**index.wxss**/
page {
  background-color: #F6FAFF;
}

.tabar {
  display: flex;
  height: 25px;
  line-height: 25px;
  padding: 0 40px 0 15px;
  text-align: center;
  font-size: 13px;
}

.tabar .tabar-img {
  width: 25px;
  height: 25px;
}
.tabar view{
  flex: 1;
}

.module-container {
  flex-wrap: wrap;
  display: flex;
  justify-content: space-between;
  padding: 0 32rpx;
}
.module-content {
  position: relative;
  width: 208rpx;
}

.module-content_hover {
  background-image: url('data:image/png;base64,');
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}

.module-content .module-img {
  width: 208rpx;
  height: 208rpx;
}

.module-content .desc-title {
  text-align: center;
  font-size: 28rpx;
  height: 64rpx;
  line-height: 64rpx;
  margin-bottom: 24rpx;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.tiShi {
  background-color: #f5b163;
  /* color: #f87e0c; */
  height: 80rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tiShi text {
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.shang {
  height: 100rpx;
  font-weight: bold;
  display: flex;
  justify-content: center;
  align-items: center;
}

.xia {
  display: flex;
  justify-content: center;
  align-items: center;
}